{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "provision": {
                    "apt-get": ["libmagic-dev"]
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}